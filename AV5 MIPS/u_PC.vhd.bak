-- PC MIPS monocycle
library ieee;
use ieee.std_logic_1164.all;
use ieee.numeric_std.all;
use ieee.std_logic_unsigned.all;

entity u_PC is
port (
      i_CLK    : in std_logic;
		i_RESET  : in std_logic;
		i_PC_IN  : in std_logic_vector (31 downto 0);
	   o_PC_OUT : out std_logic_vector (31 downto 0)
	  );
end u_PC;
